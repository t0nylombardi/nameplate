class Logger
  # Allow positional logdev as IO/String/Pathname like Ruby does.
  def initialize: (IO logdev, ?shift_age: Integer | String, ?shift_size: Integer) -> void
                | (String | Pathname logdev, ?shift_age: Integer | String, ?shift_size: Integer) -> void

  # Common instance writer methods used in code.
  attr_accessor level: untyped
  attr_accessor formatter: untyped

  # Severity constants referenced in code.
  DEBUG: Integer
  INFO: Integer
end

